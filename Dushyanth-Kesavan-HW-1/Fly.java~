public class Fly extends Creature
{
    public Fly(String name)
    {
        super(name);
    }
    public void eat(Thing aThing)
    {
    }
    public void move()
    {
        fly();
    }
    public void fly()
    {
        System.out.println(name+" "+getClass().getSimpleName()+" is buzzing around in flight.");
    }
}